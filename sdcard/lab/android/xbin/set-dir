# Set Android lab dirs

export MODS="/sdcard/lab/android/mods"
export ETC="/sdcard/lab/android/etc"
export RUN="/sdcard/lab/android/run"
export LOG="/sdcard/lab/android/log"
export VAR="/sdcard/lab/android/var"
export BIN="/sdcard/lab/android/bin"
export XBIN="/sdcard/lab/android/xbin"
export TMP="/sdcard/lab/android/tmp"

# Systemless bind-mounts
export BINDS="/data/binds"

# Magisk Magic Mounts
export SYSTEM="/sbin/.magisk/mirror/system"
export VENDOR="/sbin/.magisk/mirror/vendor"
